{
    "name": "<%= _.slugify(appName) %>",
    "version": "0.0.0",
    "private": true,
    "dependencies": {
        "angular": "<%= angularVersion %>",
        "angular-route": "<%= angularVersion %>",
        "restangular": "<%= restangularVersion %>",
        "jquery": "<%= jqueryVersion %>",
        "bootstrap": "<%= bootstrapVersion %>"
    },
    "devDependencies": {
        "angular-mocks": "<%= angularVersion %>",
        "jasmine-jquery": "1.7.0"
    },
    "exportsOverride": {
        "angular": {
            "js": "angular.min.js"
        },
        "angular-route": {
            "js": "angular-route.min.js"
        },
        "restangular": {
            "js": "dist/restangular.min.js"
        },
        "lodash": {
            "js": "dist/lodash.min.js"
        },
        "jquery": {
            "js": "jquery.min.js"
        },
        "bootstrap":{
            "js": "dist/js/bootstrap.min.js",
            "css": "dist/css/*.min.css",
            "fonts":"dist/fonts/*"
        }        
    }
}

